
<%= tinymce %>


<%= simple_form_for(@infopage) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :seotitle %>
    <%= f.input :seodesc %>
    <%= f.input :seokeywords %>
    <%= f.input :h1 %>
    <%= f.input :title %>
    <%= f.input :description, :input_html =>{ :class=>"test", :rows => 40, :cols => 120 } %>
    <%= f.input :tag %>
    <%= f.input :top %>
    <%= f.input :column %>
    <%= f.input :url %>
    <%= f.association :category %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
