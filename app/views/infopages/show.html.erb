<p id="notice"><%= notice %></p>


<div class="title">
  <h1> <%= @infopage.title %> </h1>
</div>

<div>
  <%= @infopage.description %>
</div>

<% if current_user.try(:isAdmin?)   %>
  <p>
    <b>Seotitle:</b>
    <%= @infopage.seotitle %>
  </p>

  <p>
    <b>Seodesc:</b>
    <%= @infopage.seodesc %>
  </p>

  <p>
    <b>Seokeywords:</b>
    <%= @infopage.seokeywords %>
  </p>
  <p>
    <b>Top:</b>
    <%= @infopage.top %>
  </p>

  <p>
    <b>Column:</b>
    <%= @infopage.column %>
  </p>

  <p>
    <b>Category:</b>
    <%= @infopage.category %>
  </p>

  <%= link_to 'Edit', edit_infopage_path(@infopage) %> |
  <%= link_to 'Back', infopages_path %>
<% end %>