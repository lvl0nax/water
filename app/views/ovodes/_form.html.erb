<%= simple_form_for(@ovode) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :seotitle %>
    <%= f.input :seodesc %>
    <%= f.input :seokeywords %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :top %>
    <%= f.input :column %>
    <%= f.input :tag %>
    <%= f.input :h1 %>
    <%= f.input :url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
