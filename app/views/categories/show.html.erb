<p id="notice"><%= notice %></p>



<div class="title">
  <h1> <%= @category.title %> </h1>
</div>

<div>
  <%= @category.description %>
</div>

<% if current_user.try(:isAdmin?)   %>
  <p>
    <b>Seotitle:</b>
    <%= @category.seotitle %>
  </p>

  <p>
    <b>Seodesc:</b>
    <%= @category.seodesc %>
  </p>

  <p>
    <b>Seokeywords:</b>
    <%= @category.seokeywords %>
  </p>

  <%= link_to 'Edit', edit_category_path(@category) %> |
  <%= link_to 'Back', categories_path %>
<% end %>