
<h1>Listing accessories</h1>

<table>
  <tr>
    <th>Seotitle</th>
    <th>Seodesc</th>
    <th>Seokeywords</th>
    <th>Title</th>
    <th>Description</th>
    <th>Availability</th>
    <th>Price</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @accessories.each do |accessory| %>
  <tr>
    <td><%= accessory.seotitle %></td>
    <td><%= accessory.seodesc %></td>
    <td><%= accessory.seokeywords %></td>
    <td><%= accessory.title %></td>
    <td><%= accessory.description %></td>
    <td><%= accessory.availability %></td>
    <td><%= accessory.price %></td>
    <td><%= link_to 'Show', accessory %></td>
    <td><%= link_to 'Edit', edit_accessory_path(accessory) %></td>
    <td><%= link_to 'Destroy', accessory, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Accessory', new_accessory_path %>
