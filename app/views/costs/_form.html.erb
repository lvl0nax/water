<%= simple_form_for(@cost) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :waterbottle %>
    <%= f.input :startcount %>
    <%= f.input :endcount %>
    <%= f.input :price %>
    <%= f.input :corporate %>
    <%= f.input :individual %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
